package tricky;

public class A {

	public synchronized void m1(){
		try {
			System.out.println("Thread Name : "+Thread.currentThread().getName());
			Thread.sleep(5000);
			System.out.println("Thread Name : "+Thread.currentThread().getName());
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	} 
}
